{
    "outputFile": "out.yml",
    "prefix": "your",
    "postgres": {
        "template": "./postgres_db/docker-compose.yml",
        "env": {
            "config": "./postgres_db/envConfig.json"
        },
        "args": {
            "isDB": true,
            "data_dir": "./postgres_data/"
        }
    },
    "couch": {
        "env": {
            "file": "_couch.env"
        },
        "template": "./couch_db/docker-compose.yml",
        "args": {
            "isDB": true
        }
    },
    "nginx-proxy": {
        "template": "./nginx-proxy/docker-compose.yml",

        "args": {
            "certs_dir": "_certs",
            "vhost_dir": "_vhost"
        }
    },
    "nodejs": {
        "template": "./deep/path/nodejs/docker-compose.yml",
        "env": {
            "config": "./deep/path/nodejs/src/envConfig.json",
            "pgDir": "../../../../postgres_db",
            "pgPort": "5432",
            "pgDataDir": "./postgres_data/"
        },
        "args": {
            "server_code_dir": "nodejsCode"
        }
    }
}
